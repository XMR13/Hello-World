1.  Kita menjalankan sebuah website dan ingin melihat berapa banyak uang yang kita dapat per hari melalui iklan. Buatlah sebuah program 
menggunakan struct yang dapat menampilkan semua hal yang kita inginkan dalam sebuah ad seperti (berapa banyak iklan yang diklik,
berapa persen iklan yang diklik, dll)

Jawab:

#include <iostream>

struct Iklan
{
	int iklan{};
	double pIklan{};
	double rataIklan{};
};


void printIklan(Iklan hasilIklan)
{

	std::cout << "hasil dari iklan adalah: " << hasilIklan.iklan << '\n';
	std::cout << "persenn dari diklik adalah " << hasilIklan.pIklan << '\n';
	std::cout << "rata - rata dari iklan adalah " << hasilIklan.rataIklan << '\n';
}

int calculate(Iklan iklan)
{

	return (iklan.iklan * iklan.pIklan / 100 * iklan.rataIklan);
}

int main()
{
	Iklan lmfao;
	std::cout << "masukkan banyak iklan yang diklik ";
	std::cin >> lmfao.iklan;
	std::cout << "masukkan banyak persen dari iklan yang diketik ";
	std::cin >> lmfao.pIklan;
	std::cout << "masukkan rata - rata iklan yang diklik: ";
	std::cin >> lmfao.rataIklan;



	Iklan joeMama{ lmfao.iklan, lmfao.pIklan, lmfao.rataIklan };
	printIklan(joeMama);
	std::cout << "total hari ini adalah " << calculate(joeMama);


}

