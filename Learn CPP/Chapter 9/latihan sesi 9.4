1.  Kita menjalankan sebuah website dan ingin melihat berapa banyak uang yang kita dapat per hari melalui iklan. Buatlah sebuah program 
menggunakan struct yang dapat menampilkan semua hal yang kita inginkan dalam sebuah ad seperti (berapa banyak iklan yang diklik,
berapa persen iklan yang diklik, dll)

#include <iostream >
#include <string>


struct Iklan
{
	double BanyakIklan{};
	double BanyakPersen{};
	double Ratarata{};


};

Iklan GetVal()
{
	Iklan UntukNilai;
	std::cout << "masukkan nilai jumlah iklan untuk hari ini: ";
	std::cin >> UntukNilai.BanyakIklan;
	std::cout << "masukkan nilai persentase iklan yang diklik: ";
	std::cin >> UntukNilai.BanyakPersen;
	std::cout << "rata - rata iklan hari ini: ";
	std::cin >> UntukNilai.Ratarata;

	return UntukNilai;
}

void PrintNilai(Iklan UntukPrint)
{

	std::cout << "Banyak iklan yang diklik adalah: " << UntukPrint.BanyakIklan << std::endl;
	std::cout << "Besar persentase iklan yang diklik adalah : " << UntukPrint.BanyakPersen << std::endl;
	std::cout << "Rata - rata iklan hari ini adalah : " << UntukPrint.Ratarata << std::endl;

	std::cout << "dan total untuk hari itu adalah ";
	std::cout << (UntukPrint.BanyakIklan * (UntukPrint.BanyakPersen / 100) * UntukPrint.Ratarata);
}

int main()
{
	Iklan Total{ GetVal() };
	PrintNilai(Total);
	return 0;
}

